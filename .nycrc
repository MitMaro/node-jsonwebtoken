{
  "lines": 100,
  "statements": 100,
  "functions": 100,
  "branches": 100,
  "include": [
    "**/*.js"
  ],
  "exclude": [
    "test/**/*.js",
    "tests/**/*.js"
  ],
  "reporter": [
    "json",
    "text",
    "text-summary",
    "html"
  ],
  "all": true,
  "check-coverage": true,
  "produce-source-map": false,
  "report-dir": "./coverage",
  "sourceMap": false,
  "instrument": true
}